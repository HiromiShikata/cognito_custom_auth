{
  "name": "api",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "format:code": "eslint '**/*.{js,ts}' --fix && prettier '**/*.{js,ts}' --write",
    "format:json": "prettier '**/*.json' --parser json --write",
    "format:md": "prettier '**/*.md' --parser markdown --write",
    "format": "npm run format:code && npm run format:json && npm run format:md",
    "build": "rm -rf dist && tsc",
    "test": "jest --watch",
    "report": "JEST_JUNIT_OUTPUT_DIR=./reports/junit JEST_HTML_REPORTER_OUTPUT_PATH=./reports/html/test-report.html jest --collect-coverage --ci --runInBand --reporters=default --reporters=jest-junit --reporters=jest-html-reporter",
    "deploy": "AWS_SDK_LOAD_CONFIG=1 AWS_DEFAULT_REGION=us-east-1 npx serverless deploy"
  },
  "engines": {
    "node": ">=14.15.0"
  },
  "dependencies": {
    "aws-sdk": "^2.867.0"
  },
  "devDependencies": {
    "@serverless/typescript": "^2.23.0",
    "@types/aws-lambda": "^8.10.71",
    "@types/jest": "^26.0.21",
    "@types/node": "^14.14.25",
    "@types/supertest": "^2.0.10",
    "@types/webpack-node-externals": "^2.5.0",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "eslint": "^7.22.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "jest-html-reporter": "^3.3.0",
    "jest-junit": "^12.0.0",
    "json-schema-to-ts": "^1.5.0",
    "openapi-examples-validator": "^4.3.3",
    "prettier": "^2.2.1",
    "serverless": "^2.23.0",
    "serverless-webpack": "^5.3.5",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.4",
    "ts-loader": "^8.0.15",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "typescript": "^4.1.3",
    "webpack": "^5.20.2",
    "webpack-node-externals": "^2.5.2"
  },
  "author": "HiromiShikata",
  "license": "MIT"
}
